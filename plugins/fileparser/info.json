{
  "name": "fileparser",
  "version": "0.1.2",
  "authors": [
    "arcadianCdr"
  ],
  "description": "Automatically extracts scene details from your library's file and directory structure.",
  "pluginEvents": ["sceneCreated", "sceneCustom"],
  "arguments": [
    {
      "name": "dry",
      "type": "Boolean",
      "required": false,
      "default": false,
      "description": "Whether to commit data changes."
    },
    {
      "name": "parseDate",
      "type": "Boolean",
      "required": false,
      "default": true,
      "description": "Whether to parse release dates."
    }
  ]
}